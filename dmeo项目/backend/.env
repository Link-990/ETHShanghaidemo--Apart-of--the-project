# 端口配置
PORT=3001

# Web3 Provider URLs (Holesky测试网配置)
WEB3_PROVIDER_URL=https://ethereum-holesky-rpc.publicnode.com

# Holesky测试网配置
CHAIN_ID=17000

# Qwen API 配置 (请替换为你的真实API密钥)
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_MODEL=qwen-plus

# MongoDB 配置 (请替换为你的数据库连接字符串)
MONGODB_URI=mongodb://localhost:27017/your_database_name

# 智能合约地址 (部署后请更新为实际地址)
RISK_ANALYZER_CONTRACT_ADDRESS=your_risk_analyzer_contract_address
AI_RISK_CONTROLLER_CONTRACT_ADDRESS=your_ai_risk_controller_contract_address
CONTRACT_DEPLOYMENT_BLOCK=0